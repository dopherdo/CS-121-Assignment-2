        "cell",
        "diseases,",
        "including",
        "leukemias,",
        "are",
        "caused",
        "by",
        "malfunctions",
        "in",
        "some",
        "part",
        "of",
        "the",
        "hematopoiesis",
        "process.\u201d",
        "Professor",
        "Minin",
        "is",
        "developing",
        "methods",
        "for",
        "analyzing",
        "high",
        "throughput",
        "experimental",
        "techniques",
        "that",
        "allow",
        "us",
        "to",
        "peek",
        "inside",
        "the",
        "hematopoiesis",
