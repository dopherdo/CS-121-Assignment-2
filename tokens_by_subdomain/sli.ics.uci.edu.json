        "the",
        "model.",
        "Statistical",
        "models",
        "for",
        "event",
        "detection",
        "in",
        "count",
        "data",
        "--Automatic",
        "event",
        "detection",
        "in",
        "data",
        "consisting",
        "of",
        "discrete",
        "counts,",
        "using",
        "variants",
        "of",
        "Poisson",
        "processes",
        "to",
        "model",
        "the",
        "underlying",
        "(non-event)",
        "phenomena.",
        "The",
